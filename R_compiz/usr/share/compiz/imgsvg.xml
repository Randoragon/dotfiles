<?xml version="1.0" encoding="UTF-8"?>
<compiz>
    <plugin useBcop="true" name="imgsvg">
	<short>SVG</short>
	<long>SVG image loader</long>
	<category>Image Loading</category>
	<feature>imageext:svg</feature>
	<feature>imagemime:image/svg+xml</feature>
	<deps>
	    <relation type="after">
		<plugin>composite</plugin>
		<plugin>opengl</plugin>
		<plugin>decor</plugin>
	    </relation>
	</deps>
	<options>
	    <option type="action" name="set">
		<short>Set overlay</short>
		<long>Set window overlay</long>
		<allowed/>
		<default/>
	    </option>
	</options>
    </plugin>
</compiz>