# File for aliases that take you places
# The directories section is read by genlfjmp script

# Files
alias jmp='vim ~/.config/jumprc'
alias als='vim ~/.config/aliasrc'
alias mml='vim ~/.config/mimeapps.list'
alias pro='vim ~/.profile'
alias xrc='vim ~/.xinitrc'
alias xpr='vim ~/.xprofile'
alias zrc='vim ~/.zshrc'
alias vrc='vim ~/.vimrc'
alias api='vim ~/dotfiles/H_scripts/.scripts/arch_postinstall.sh'
alias xbk='vim ~/.xbindkeysrc'
alias xres='vim ~/.Xresources'

# Directories
alias cdoc='cd ~/Documents && ls'
alias cdpi='cd ~/Pictures && ls'
alias cdmu='cd ~/Music && ls'
alias cdvi='cd ~/Videos && ls'
alias cdpr='cd ~/Projects && ls'
alias cdso='cd ~/Software && ls'
alias cdow='cd ~/Downloads && ls'
alias cdowmu='cd ~/Downloads/Music && ls'
alias cdot='cd ~/dotfiles && ls'
alias cdro='cd ~/Dropbox && ls'
alias cdme='cd ~/MEGA && ls'
alias cdap='cd ~/.local/share/applications && ls'
alias cdrap='cd /usr/share/applications && ls'
alias cdsc='cd ~/.scripts && ls'
alias cdls='cd ~/.local/bin && ls'
alias cdrs='cd /usr/local/bin && ls'
alias cddap='cd ~/dotfiles/H_mime/.local/share/applications && ls'
alias cddsc='cd ~/dotfiles/H_scripts/.scripts && ls'
alias cddls='cd ~/dotfiles/H_scripts/.local/bin && ls'
alias cddrs='cd ~/dotfiles/R_scripts/usr/local/bin && ls'
alias cdtr='cd ~/.local/share/Trash/files && ls'
alias cdmn='cd /mnt && ls'
alias cdwall='cd ~/.local/share/wallpapers && ls'
alias cdst='cd ~/Software/st && ls'
